        "electron-json-storage": "^2.0.0",
        "lodash": "^4.13.1"
    },
    "devDependencies": {
        "appdmg": "^0.4.5",
        "electron-builder": "^4.2.0",
        "electron-debug": "^1.0.0",
        "electron-packager": "^7.0.3",
        "electron-prebuilt": "^1.2.0"
    },
    "build": {
        "platform": [
            "darwin", "win32", "linux"
        ],
        "directory": "./build",
        "productName": "Pushbullet for Desktop",
        "id": "de.sidneys.pushbullet-desktop",
        "electron": "1.2.1",
        "number": 1,
        "category": "public.app-category.utilities"
