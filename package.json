{
    "name": "pushbullet-desktop",
    "version": "0.8.0",
    "description": "'Pushbullet for Desktop' is a lightweight Pushbullet Client for OS X, Windows & Linux, providing cross-platform native system notifications in an unobstrusive way.",
    "license": "MIT",
    "main": "index.js",
    "author": {
        "name": "sidneys",
        "url": "http://sidneys.github.io/pushbullet-desktop"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/sidneys/pushbullet-desktop.git"
    },
    "bugs": {
        "url": "https://github.com/sidneys/pushbullet-desktop/issues"
    },
    "homepage": "http://sidneys.github.io/pushbullet-desktop",
    "keywords": [
        "Pushbullet, Push, Notifications, Desktop, native, Electron, node, npm, Chromium, Mac, Windows, Windows 10, Linux,  Pushover"
    ],
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "debug": "electron .",
        "build": "node build.js -- "
    },
